{
  "expo": {
    "name": "eas-workflow-playground-local",
    "slug": "eas-workflow-playground-local",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "easworkflowplaygroundlocal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hsatacexpo.easworkflowplaygroundlocal2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fd363c72-4299-4a72-9f70-19d62a810af1"
      }
    },
    "owner": "testuser",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "http://127.0.0.1:3000/expo-updates/fd363c72-4299-4a72-9f70-19d62a810af1"
    }
  }
}
